{
    "name": "fm-labs/cakephp-mailchimp",
    "description": "Mailchimp plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Mailchimp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailchimp\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
